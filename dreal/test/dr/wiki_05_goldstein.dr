# From: https://en.wikipedia.org/wiki/Test_functions_for_optimization
# prec: 1

var:
    [-2, 2] x;
    [-2, 2] y;

# Global minimum is f(0, -1) = 3

cost:
    (1 + (x + y + 1)^2 * (19 - 14*x + 3*x^2 - 14*y + 6*x*y + 3*y^2)) *
    (30 + (2*x - 3*y)^2 * (18 - 32*x + 12*x^2 + 48*y - 36*x*y + 27*y^2));
