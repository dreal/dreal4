# From: https://en.wikipedia.org/wiki/Test_functions_for_optimization
# prec: 0.0001

var:
    [-10, 10] x;
    [-10, 10] y;

# Global minimum is:
#     f( 1.34941, -1.34941) = -2.06261
#     f( 1.34941,  1.34941) = -2.06261
#     f(-1.34941, -1.34941) = -2.06261
#     f(-1.34941,  1.34941) = -2.06261

cost:
    -0.0001 * (abs(sin(x) * sin(y) * exp(abs(100 - sqrt(x^2 + y^2) / 3.141592))) + 1)^0.1;
